  "bugs": {
    "url": "https://github.com/vcgtz/generator-module-npm/issues"
  },
  "homepage": "https://github.com/vcgtz/generator-module-npm#readme",
  "dependencies": {
    "yeoman-generator": "^5.7.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
